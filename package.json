{
  "name": "epc",
  "private": true,
  "scripts": {
    "tr": "turbo run",
    "postinstall": "turbo gql:pregen gql:gen",
    "gql:gen": "turbo gql:gen",
    "test": "turbo test",
    "build": "turbo build",
    "push": "sst deploy",
    "dev": "turbo dev:sst dev:gql",
    "dev-client": "turbo dev-client",
    "dev-email": "turbo dev:email",
    "dev:sst": "sst dev --stage=development --mode=mono"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "overrides": {
      "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
      "body-parser@<1.20.3": ">=1.20.3",
      "braces@<3.0.3": ">=3.0.3",
      "cookie@<0.7.0": ">=0.7.0",
      "dset@<3.1.4": ">=3.1.4",
      "express@<4.20.0": ">=4.20.0",
      "jsonpath-plus@<10.0.0": ">=10.0.0",
      "linkifyjs@<4.3.2": ">=4.3.2",
      "micromatch@<4.0.8": ">=4.0.8",
      "next@>=14.0.0 <14.2.10": ">=14.2.10",
      "path-to-regexp@<0.1.10": ">=0.1.10",
      "send@<0.19.0": ">=0.19.0",
      "serve-static@<1.16.0": ">=1.16.0",
      "svelte@<4.2.19": ">=4.2.19",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "aws-sdk",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "sst": "catalog:"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.149",
    "turbo": "^2.5.3",
    "typescript": "catalog:"
  }
}